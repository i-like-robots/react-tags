{
  "name": "react-tag-autocomplete",
  "version": "5.4.1",
  "description": "React Tag Autocomplete is a simple tagging component ready to drop in your React projects.",
  "main": "dist/ReactTags-cjs.js",
  "module": "dist/ReactTags-es6.js",
  "scripts": {
    "pretest": "npm run lint && npm run build:cjs",
    "test": "jasmine",
    "coverage": "istanbul cover -i 'dist-cjs/**' jasmine",
    "lint": "tslint -c tslint.json src/*.tsx spec/*.js",
    "dev": "webpack-dev-server --progress --colors --port ${PORT:-8080} --host 0.0.0.0",
    "build:example": "webpack",
    "build:cjs": "tsc --outDir dist-cjs -m commonjs",
    "build:es6": "tsc --outDir dist-es6",
    "prepublish": "npm run build:cjs && npm run build:es6"
  },
  "files": [
    "lib",
    "example",
    "dist-cjs",
    "dist-es6",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "tags",
    "tag input",
    "react-component",
    "autosuggest",
    "autocomplete"
  ],
  "author": "Matt Hinchliffe",
  "contributors": [
    "Prakhar Srivastav",
    "Simon HÃ¶tten",
    "Andre-John Mas",
    "Mike Kamermans"
  ],
  "license": "MIT",
  "repository": "https://github.com/i-like-robots/react-tags",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.4",
    "jasmine": "^2.4.1",
    "jsdom": "^11.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sinon": "^4.0.0",
    "ts-loader": "^3.2.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}
